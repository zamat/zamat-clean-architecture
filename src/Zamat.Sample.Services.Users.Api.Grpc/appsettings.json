{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "LocalizerOptions": {
    "DefaultCulture": "en-US",
    "SupportedCultures": [ "en-GB", "en-US", "pl-PL" ],
    "SupportedUICultures": [ "en-GB", "en-US", "pl-PL" ]
  }
}
