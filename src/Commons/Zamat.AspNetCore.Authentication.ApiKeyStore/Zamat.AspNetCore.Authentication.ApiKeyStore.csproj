<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>$(CommonVersion)</Version>
    <TargetFramework>$(DefaultTargetFramework)</TargetFramework>
    <Description>AUMS Common AspNetCore Authentication ApiKey</Description>
    <PackageTags>Authentication;ApiKey</PackageTags>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AUMS.AspNetCore.Authentication.ApiKey\AUMS.AspNetCore.Authentication.ApiKey.csproj" />
    <ProjectReference Include="..\AUMS.AspNetCore.Authorization\AUMS.AspNetCore.Authorization.csproj" />
  </ItemGroup>

</Project>
